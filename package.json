{
  "name": "ne-schemata",
  "version": "2.2.2",
  "description": "GraphQL IDL/Schemata String class to make working with GraphQL easier",
  "repository": "https://github.com/nyteshade/ne-schemata",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "build": "babel src --out-dir dist"
  },
  "keywords": [
    "SDL",
    "IDL",
    "AST",
    "utility",
    "GraphQLSchema",
    "GraphQL"
  ],
  "author": "Brielle Harrison",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-classes": "^7.8.6",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-flow": "^7.8.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.0",
    "bili": "^4.8.1",
    "eslint": "^6.8.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^5.0.0",
    "eslint-plugin-import": "^2.20.1",
    "flow-bin": "^0.103.0",
    "graphql": "^14.6.0",
    "minimist": "^1.2.2",
    "jest": "^25.1.0",
    "jest-environment-node-debug": "^2.0.0",
    "serialize-javascript": "^2.1.1",
    "regenerator-runtime": "^0.13.5"
  },
  "peerDependencies": {
    "graphql": "^14"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "graphql",
      "json",
      "jsx"
    ],
    "verbose": true,
    "transform": {
      "^.+\\.graphql": "<rootDir>/jestTransformer",
      "^.+\\.jsx?o?n?$": "babel-jest"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.8.7",
    "debug": "^4.1.1",
    "deepmerge": "^4.0.0",
    "ne-tag-fns": "^0.6.9",
    "pretty-error": "^2.1.1"
  }
}
