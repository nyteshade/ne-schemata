{
    "name": "ne-schemata",
    "version": "2.0.0",
    "description": "GraphQL IDL/Schemata String class to make working with GraphQL easier",
    "repository": "https://github.com/nyteshade/ne-schemata",
    "main": "dist/index.js",
    "scripts": {
        "test": "jest --no-cache --runInBand",
        "build": "rm -rf dist/* ; babel -s --source-root src src/ -d dist/"
    },
    "keywords": [
        "SDL",
        "IDL",
        "AST",
        "utility",
        "GraphQLSchema",
        "GraphQL"
    ],
    "author": "Brielle Harrison",
    "license": "MIT",
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-eslint": "^9.0.0",
        "babel-jest": "^23.6.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-preset-env": "^1.6.1",
        "babel-preset-flow": "^6.23.0",
        "eslint": "^5.10.0",
        "eslint-plugin-babel": "^5.3.0",
        "eslint-plugin-flowtype": "^3.0.0",
        "eslint-plugin-import": "^2.14.0",
        "flow-bin": "^0.89.0",
        "graphql": "^0.13",
        "jest": "^22.4.3",
        "jest-environment-node-debug": "^2.0.0"
    },
    "peerDependencies": {
        "graphql": "^0.13 || ^0.14"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "graphql",
            "json",
            "jsx"
        ],
        "verbose": true,
        "transform": {
            "^.+\\.graphql": "<rootDir>/jestTransformer",
            "^.+\\.jsx?o?n?$": "babel-jest"
        }
    },
    "dependencies": {
        "debug": "^4.1.1",
        "deepmerge": "^3.0.0",
        "ne-tag-fns": "^0.6.8",
        "pretty-error": "^2.1.1"
    }
}
