{
  "name": "@nyteshade/ne-schemata",
  "version": "3.6.1",
  "description": "GraphQL IDL/Schemata String class to make working with GraphQL easier",
  "repository": "https://github.com/nyteshade/ne-schemata",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "build": "babel src --out-dir dist"
  },
  "engines": {
    "node": ">=19.0.0"
  },
  "keywords": [
    "SDL",
    "IDL",
    "AST",
    "utility",
    "GraphQLSchema",
    "GraphQL"
  ],
  "author": "Brielle Harrison",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.6",
    "@babel/plugin-proposal-decorators": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-flow": "^7.23.3",
    "babel-jest": "^29.7.0",
    "eslint": "^8.55.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.29.1",
    "flow-bin": "^0.224.0",
    "graphql": "^16",
    "jest": "^29.7.0"
  },
  "peerDependencies": {
    "graphql": "^16"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "graphql",
      "json",
      "jsx"
    ],
    "verbose": true,
    "transform": {
      "^.+\\.graphql": "<rootDir>/jestTransformer",
      "^.+\\.jsx?o?n?$": "babel-jest"
    }
  },
  "dependencies": {
    "@graphql-tools/utils": "^10.0.11",
    "@babel/eslint-parser": "^7.23.3",
    "core-js": "^3.34.0",
    "debug": "^4.3.4",
    "deepmerge": "^4.3.1",
    "flow-parser": "^0.224.0",
    "lodash": "^4.17.21",
    "ne-tag-fns": "^0.7.0",
    "pretty-error": "^4.0.0"
  }
}
